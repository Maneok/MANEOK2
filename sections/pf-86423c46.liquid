(() => {
  const LS_KEY = 'theme_selection';
  const PREVIEW = '#theme-preview';
  const nextBtn = document.getElementById('next-step');
  const EMBEDS = { /* ‚Ä¶ m√™me objet qu‚Äôavant ‚Ä¶ */ };

  /* Helpers */
  const $ = s => document.querySelector(s);
  const $$ = s => document.querySelectorAll(s);

  /* Render banner -------------------------------------------------- */
  function render(themeId) {
    const cont = $(PREVIEW);
    if (!cont) return;

    if (themeId && EMBEDS[themeId]) {
      cont.innerHTML =
        `<iframe src="${EMBEDS[themeId]}" loading="lazy" allowfullscreen
                 style="width:100%;aspect-ratio:16/9;border:0;border-radius:8px;"
                 title="Aper√ßu visuel du th√®me"></iframe>`;
      cont.classList.remove('placeholder');
    } else {
      cont.innerHTML =
        '<p style="text-align:center;color:#888;font-size:14px;margin:0;">Aucun th√®me s√©lectionn√©.</p>';
      cont.classList.add('placeholder');
    }
    /*  D√©sactive/active le bouton suivant */
    if (nextBtn) nextBtn.disabled = !themeId;
  }

  /* Init picker ---------------------------------------------------- */
  function initPicker() {
    const radios = $$('input[name="theme"]');
    if (!radios.length) return;

    radios.forEach(r => {
      r.addEventListener('change', () => {
        localStorage.setItem(LS_KEY, r.value);
        render(r.value);
      });
    });

    const saved = localStorage.getItem(LS_KEY);
    if (saved) {
      const pre = $(`input[name="theme"][value="${saved}"]`);
      if (pre) pre.checked = true;
    }
  }

  /* Listen ‚Äústorage‚Äù¬†‚Üí sync multi‚Äëonglets -------------------------- */
  window.addEventListener('storage', e => {
    if (e.key === LS_KEY) render(e.newValue);
  });

  /* pageshow¬†: ressort BFCache ------------------------------------ */
  window.addEventListener('pageshow', () => {
    render(localStorage.getItem(LS_KEY));
  });

  /* Guard bouton (fallback, au cas o√π) ----------------------------- */
  if (nextBtn) {
    nextBtn.addEventListener('click', e => {
      if (!localStorage.getItem(LS_KEY)) {
        e.preventDefault();
        alert('Choisissez un th√®me avant de continuer.');
      }
    });
  }

  /* Run */
  document.addEventListener('DOMContentLoaded', () => {
    initPicker();
    render(localStorage.getItem(LS_KEY));
  });
})();







(() => {
  const barFill = document.querySelector('.progress-fill');
  const mainEl = document.getElementById('wizard');
  const progressBar = document.getElementById('progress-bar');
  const markers = document.querySelectorAll('.marker');
  const label = document.querySelector('.progress-label');

  const step = parseInt(mainEl.dataset.step, 10) || 1;
  const total = parseInt(mainEl.dataset.total, 10) || 5;
  const percent = Math.round((step / total) * 100);

  // Remplir la barre
  if (barFill) {
    barFill.style.width = `${percent}%`;
  }

  // Mise √† jour ARIA et label
  if (progressBar) {
    progressBar.setAttribute('aria-valuenow', percent);
    progressBar.setAttribute('aria-valuetext', `√âtape ${step} sur ${total}`);
  }
  if (label) {
    label.textContent = `√âtape ${step}/${total}`;
  }

  // Activer markers jusqu'√† l'√©tape courante
  markers.forEach(marker => {
    const mStep = parseInt(marker.dataset.step, 10);
    if (mStep <= step) marker.classList.add('active');
  });

  // Effet pulse sur le marker actuel
  const currentMarker = document.querySelector(`.marker[data-step="${step}"]`);
  if (currentMarker) {
    const pulse = () => {
      currentMarker.animate([
        { boxShadow: '0 0 0px rgba(0,0,0,0)' },
        { boxShadow: `0 0 8px var(--primary-color)` },
        { boxShadow: '0 0 0px rgba(0,0,0,0)' }
      ], { duration: 1200, easing: 'ease-in-out' });
    };
    // D√©clencher le premier pulse l√©g√®rement apr√®s l'expansion
    setTimeout(pulse, 2100);
    // R√©p√©ter le pulse r√©guli√®rement
    setInterval(pulse, 5000);
  }
})();</script>
