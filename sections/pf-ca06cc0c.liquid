/* ========== 6. SAUVEGARDE DES OPTIONS DE CETTE PAGE ================ */
function getCustom() {
  return {
    images: $('input[name="q1"]:checked')?.value || '',
    texts: $('input[name="q2"]:checked')?.value || '',
    changeColor: $('input[name="q3"]:checked')?.value || '',
    primaryColor: $('input[name="primaryColor"]')?.value || '',
    extraChoice: $('input[name="q4"]:checked')?.value || '',
    extraNotes: $('textarea[name="extraNotes"]')?.value.trim() || ''
  };
}
function saveCustom() {
  localStorage.setItem(LS_CUSTOM, JSON.stringify(getCustom()));
}
function restoreCustom() {
  try {
    const c = JSON.parse(localStorage.getItem(LS_CUSTOM) || '{}');
    if (!Object.keys(c).length) return;

    /* radios */
    ['q1', 'q2', 'q3', 'q4'].forEach(q => {
      if (c[q === 'q3' ? 'changeColor' : q === 'q4' ? 'extraChoice' : q.slice(0, 2)]) {
        const val = c[q === 'q3' ? 'changeColor' : q === 'q4' ? 'extraChoice' : q.slice(0, 2)];
        const radio = $(`input[name="${q}"][value="${val}"]`);
        radio && (radio.checked = true);
        radio && radio.dispatchEvent(new Event('change', { bubbles: true }));
      }
    });

    /* couleur */
    if (c.primaryColor) {
      const swatch = $(`.swatch[data-color="${c.primaryColor}"]`);
      swatch && swatch.click();
    }

    /* textarea */
    if (c.extraNotes) {
      $('textarea[name="extraNotes"]').value = c.extraNotes;
    }
  } catch {/* ignore */ }
}

/* ========== 7. INTERACTIONS « OUI/NON » ET PALETTE ================ */
/* — Oui/Non + affichage conditionnel — */
$$('.options').forEach(group => {
  group.addEventListener('change', e => {
    if (!e.target.matches('input[type="radio"]')) return;

    /* surlignage */
    group.querySelectorAll('.option').forEach(opt => {
      opt.classList.toggle('is-selected', opt.querySelector('input').checked);
    });

    /* toggle bloc */
    const body = group.closest('fieldset').querySelector('.question-body');
    if (body) body.hidden = (e.target.value !== 'oui');

    saveCustom();
  });
});

/* — Palette — */
const swatches = $$('.swatch');
const colorInput = $('input[name="primaryColor"]');
swatches.forEach(btn => {
  if (btn.dataset.color === colorInput.value) btn.classList.add('is-active');
  btn.addEventListener('click', () => {
    swatches.forEach(b => b.classList.remove('is-active'));
    btn.classList.add('is-active');
    colorInput.value = btn.dataset.color;
    document.documentElement.style.setProperty('--primary-color', btn.dataset.color);
    saveCustom();
  });
});

/* — Textarea — */
const txtArea = $('textarea[name="extraNotes"]');
txtArea && txtArea.addEventListener('input', saveCustom);

/* ========== 8. MULTI-ONGLETS / RETOUR ARRIÈRE ====================== */
window.addEventListener('storage', e => {
  if (e.key === LS_THEME) renderBanner(e.newValue);
  if (e.key === LS_COMPANY) { restoreData(); toggleNext(); }
  if (e.key === LS_CUSTOM) restoreCustom();
});

window.addEventListener('pageshow', () => {
  renderBanner();
  restoreData();
  restoreCustom();
  toggleNext();
});

/* ========== 9. GARDE D’ACCÈS + INIT =============================== */
